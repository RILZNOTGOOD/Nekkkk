local library = loadstring(game:HttpGet("https://pastebin.com/raw/egMXJcwB", true))()
library.options.underlinecolor = "rainbow"

local w = library:CreateWindow('Main')

 
 local t = w:Toggle('Yellow Orb', {flag = "M"})
spawn( 
    function()
        while wait(0.0000001) do
            if w.flags.M then
                loadstring(game:HttpGet("https://raw.githubusercontent.com/RILZNOTGOOD/Nekkkk/refs/heads/main/Y"))()
            end
        end
    end
)

local t = w:Toggle('Red Orb', {flag = "S"})
spawn( 
    function()
        while wait(0.0000000001) do
            if w.flags.S then
                loadstring(game:HttpGet("https://raw.githubusercontent.com/RILZNOTGOOD/Nekkkk/refs/heads/main/R"))()
            end
        end
    end
)

local t = w:Toggle('Ethereal', {flag = "P"})
spawn( 
    function()
        while wait(0.00000001) do
            if w.flags.P then
                loadstring(game:HttpGet("https://raw.githubusercontent.com/RILZNOTGOOD/Nekkkk/refs/heads/main/E"))()
            end
        end
    end
)

local t = w:Toggle('Rebirth', {flag = "C"})
spawn( 
    function()
        while wait(0.00000000001) do
            if w.flags.C then
                 game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer(unpack(args))
game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer(unpack(args))
            end
        end
    end
)

local w = library:CreateWindow('Pet')

local t = w:Toggle('Sw', {flag = "A"})
spawn( 
    function()
        while wait() do
            if w.flags.A then
                game:GetService("ReplicatedStorage").cPetShopRemote:InvokeServer(game:GetService("ReplicatedStorage").cPetShopFolder["Swift Samurai"])
            end
        end
    end
)
